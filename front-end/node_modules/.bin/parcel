#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/randeera/Documents/dep-11/my-projects/web-apps/k-soft-customer-details-search-app/front-end/node_modules/.pnpm/parcel@2.10.3/node_modules/parcel/lib/node_modules:/home/randeera/Documents/dep-11/my-projects/web-apps/k-soft-customer-details-search-app/front-end/node_modules/.pnpm/parcel@2.10.3/node_modules/parcel/node_modules:/home/randeera/Documents/dep-11/my-projects/web-apps/k-soft-customer-details-search-app/front-end/node_modules/.pnpm/parcel@2.10.3/node_modules:/home/randeera/Documents/dep-11/my-projects/web-apps/k-soft-customer-details-search-app/front-end/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/randeera/Documents/dep-11/my-projects/web-apps/k-soft-customer-details-search-app/front-end/node_modules/.pnpm/parcel@2.10.3/node_modules/parcel/lib/node_modules:/home/randeera/Documents/dep-11/my-projects/web-apps/k-soft-customer-details-search-app/front-end/node_modules/.pnpm/parcel@2.10.3/node_modules/parcel/node_modules:/home/randeera/Documents/dep-11/my-projects/web-apps/k-soft-customer-details-search-app/front-end/node_modules/.pnpm/parcel@2.10.3/node_modules:/home/randeera/Documents/dep-11/my-projects/web-apps/k-soft-customer-details-search-app/front-end/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../parcel/lib/bin.js" "$@"
else
  exec node  "$basedir/../parcel/lib/bin.js" "$@"
fi
